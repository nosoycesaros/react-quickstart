{
  "name": "react-quickstart",
  "version": "1.0.0",
  "description": "Quickstart boilerplate for React projects",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --progress",
    "test": "mocha --compilers js:babel-core/register --require ignore-styles --require ./test/helpers.js --timeout 5000 --recursive",
    "test:watch": "npm test -- --watch",
    "build": "rimraf dist && NODE_ENV=production webpack --progress --profile --bail"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nosoycesaros/react-quickstart.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "quickstart",
    "sass",
    "webpack"
  ],
  "author": "Cesaros",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/nosoycesaros/react-quickstart/issues"
  },
  "homepage": "https://github.com/nosoycesaros/react-quickstart",
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.8.5",
    "ignore-styles": "^4.0.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.1"
  }
}
